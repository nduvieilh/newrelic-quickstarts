            "widgets": [
                {
                    "title": "Clusters: cpu utilization (%)",
                    "layout": {
                        "height": 3,
                        "width": 4,
                        "row": 1,
                        "column": 1
                    },
                    "visualization": {
                        "id": "viz.line"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 1,
                                "query": "SELECT average(`aws.ecs.CPUUtilization.byCluster`) FROM Metric since 1 day ago TIMESERIES auto facet `aws.ecs.ClusterName`"
                            }
                        ]
                    }
                },
