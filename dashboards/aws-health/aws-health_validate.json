{
    "name": "Aws health",
    "description": "",
    "pages": [
        {
            "name": "Aws health",
            "description": "",
            "widgets": [
                {
                    "title": "Open issues",
                    "layout": {
                        "height": 6,
                        "width": 8,
                        "row": 1,
                        "column": 5
                    },
                    "visualization": {
                        "id": "viz.table"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 1,
                                "query": "SELECT service, affectedRegion, description FROM AwsHealthNotification WHERE statusCode = 'open' eventTypeCategory = 'Issue'"
                            }
                        ]
                    }
                },
                {
                    "title": "Scheduled changes",
                    "layout": {
                        "height": 6,
                        "width": 8,
                        "row": 7,
                        "column": 5
                    },
                    "visualization": {
                        "id": "viz.table"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 1,
                                "query": "SELECT service, affectedRegion, description FROM AwsHealthNotification WHERE eventTypeCategory = 'ScheduledChange'"
                            }
                        ]
                    }
                },
                {
                    "title": "All issues",
                    "layout": {
                        "height": 6,
                        "width": 8,
                        "row": 13,
                        "column": 5
                    },
                    "visualization": {
                        "id": "viz.table"
                    },
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 1,
                                "query": "SELECT service, affectedRegion, startTime, statusCode, description FROM AwsHealthNotification LIMIT MAX"
                            }
                        ]
                    }
                }
            ]
        }
    ],
    "permissions": "PUBLIC_READ_WRITE"
}